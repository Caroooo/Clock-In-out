<mvc:View
        controllerName="sap.ui.demo.wt.controller.Outbox"
        xmlns:mvc="sap.ui.core.mvc"
        xmlns="sap.m"
        height="100%"
        width="100%">
    <Page
            class="sapUiContentPadding"
            showNavButton="false"
            showHeader="false">

        <Button width="100%" text="Send to Blum" press="handleSendToWebService"></Button>

        <PullToRefresh
                id="pullToRefresh"
                refresh="handleRefresh" />
        <!--<List id="outboxList"></List>-->
        <List id="outboxList" items="{path: '/savedBookings', type:'sap.ui.model.type.String', sorter : {path : 'date'}}">
            <ObjectListItem title="{type}" number="{date} {time}" />
        </List>


    </Page>

</mvc:View>