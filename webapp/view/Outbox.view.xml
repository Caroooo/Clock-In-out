<mvc:View
        controllerName="sap.ui.demo.wt.controller.Outbox"
        xmlns:mvc="sap.ui.core.mvc"
        xmlns="sap.m"
        height="100%"
        width="100%">
    <Page
            class="sapUiContentPadding"
            showNavButton="false"
            showHeader="false">

        <Button width="100%" text="Send to Blum" press="handleSendToWebService"></Button>

        <PullToRefresh
                id="pullToRefresh"
                refresh="handleRefresh"/>
        <!--<List id="outboxList"></List>-->
        <!--<List id="outboxList" items="{path: 'outbox>/', type:'sap.ui.model.type.String'}">-->
        <!--<ObjectListItem title="{outbox>type}, , ,{outbox>date}, {outbox>time}" number= "sent"/>-->
        <!--</List>-->

        <Table items="{path: 'outbox>/',type:'sap.ui.model.type.String', sorter: {path : 'outbox>date', group: true}}">
            <columns>
                <Column width="40%"></Column>
                <Column width="40%"> </Column>
                <Column hAlign="Right"></Column>
            </columns>
            <items>
                <ColumnListItem>
                    <cells>
                        <Text text="{outbox>date}"/>
                        <Text text="{outbox>time}"/>
                        <Text text="{value: outbox>type, formatter: '.formatClockType'}"/>
                    </cells>
                </ColumnListItem>
            </items>
        </Table>


    </Page>

</mvc:View>