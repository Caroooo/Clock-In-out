<mvc:View
        controllerName="sap.ui.demo.wt.controller.Main2"
        height="100%"
        xmlns:l="sap.ui.layout"
        xmlns:u="sap.ui.unified"
        xmlns:mvc="sap.ui.core.mvc"
        xmlns="sap.m"
        class="viewPadding">

    <Toolbar>
        <Button id="openMenu" icon="sap-icon://menu2" press="handlePressOpenMenu" class="menuButton" />
        <Image
                src="logo-blum.png"
                densityAware="false"
                width="5rem"/>
    </Toolbar>


            <IconTabBar
                    id="idIconTabBarSeparatorNoIcon"
                    expanded="true"
                    expandable="false">
                <items>
                    <IconTabFilter
                            text="Clock it">
                        <mvc:XMLView viewName="sap.ui.demo.wt.view.Stamp"/>
                    </IconTabFilter>
                    <IconTabFilter
                            text="Outbox">
                        <mvc:XMLView viewName="sap.ui.demo.wt.view.Outbox"/>
                    </IconTabFilter>

                </items>
            </IconTabBar>

</mvc:View>
